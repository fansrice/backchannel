<h1>Listing comment_votes</h1>

<table>
  <thead>
    <tr>
      <th>Voter</th>
      <th>Quantity</th>
      <th>Post comment</th>
      <th>User</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_votes.each do |comment_vote| %>
      <tr>
        <td><%= comment_vote.voter_id %></td>
        <td><%= comment_vote.quantity %></td>
        <td><%= comment_vote.post_comment %></td>
        <td><%= comment_vote.user %></td>
        <td><%= link_to 'Show', comment_vote %></td>
        <td><%= link_to 'Edit', edit_comment_vote_path(comment_vote) %></td>
        <td><%= link_to 'Destroy', comment_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment vote', new_comment_vote_path %>
